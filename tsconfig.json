{
  "compilerOptions": {
    /* Emit */
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    /* Completeness */
    "skipLibCheck": true
  }
}
